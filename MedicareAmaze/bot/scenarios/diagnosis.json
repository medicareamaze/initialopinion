{
  "id": "diagnosis",
  "version": "112",
  "type": "sequence",
  "steps": [


    {
      "id": "welcomeCard",
      "type": "heroCard",
      "data": {
        "title": "Welcome to DOC Bot",
        "text": "Hi There! I am Prof. Winston Davidson, Medical Doctor. My artificially intelligent bot will ask you a series of questions and based on your responses, it will provide you a pre-diagnosis and recommend the next steps. It can also help you screen for the **Corona Virus Infection**"

      }
    },
    {
      "id": "location",
      "type": "prompt",
      "data": {
        "type": "choice",
        "text": "Please select your country of residence",
        "options": [ "Jamaica", "Trinidad and Tobago", "Barbados", "Guyana"]
      }
    },
    {
      "id": "location_state",
      "type": "prompt",
      "data": {
        "type": "text",
        "text": "Enter your state/parish"       
       }
    },
    {
      "id": "location_city",
      "type": "prompt",
      "data": {
        "type": "text",
        "text": "Enter your city/town/community"       
       }
    },
    {
      "id": "location_street",
      "type": "prompt",
      "data": {
        "type": "text",
        "text": "Enter your street and house #"       
       }
    },
    {
      "id": "gender",
      "type": "prompt",
      "data": {
        "type": "choice",
        "text": "Please select your gender",
        "options": [ "male", "female" ]
      }
    },
    {
      "id": "dob",
      "type": "prompt",
      "data": {
        "type": "time",
        "text": "Enter your date of birth (mm/dd/yyyy)"
      }
    },
    
    {
      "id": "s_98",
      "type": "prompt",
      "data": {
        "type": "choice",
        "text": "Do you have fever?",
        "options": [ "Yes", "No", "Dont Know" ]
      }
    },
    {
      "id": "s_105",
      "type": "prompt",
      "data": {
        "type": "choice",
        "text": "Do you have cough?",
        "options": [ "Yes", "No", "Dont Know" ]
      }
    },
    {
      "id": "s_1462",
      "type": "prompt",
      "data": {
        "type": "choice",
        "text": "Do you have shortness of breath?",
        "options": [ "Yes", "No", "Dont Know" ]
      }
    },
   
    {
      "id": "CovidTemperature",
      "type": "sequence",
      "scenarios": [
        {
          "condition": "s_98 == 'Yes'  && !(s_98 == 'Yes' && s_1462== 'Yes')",
          "steps": [
            {
              "id": "temparature",
             
              "type": "prompt",
              "data": {
                "type": "choice",
                "text": "How high is your fever? Is your temparature greater than 99.5 F?",
                "options": [ "Yes", "No", "Dont Know. I havent taken my temperature" ]
              }
            }
          ]
        }
      ]
    },
    {
      "id": "additionalCovidSymptoms",
      "type": "sequence",
      "scenarios": [
        {
          "condition": " (s_98 == 'Yes' || s_1462 == 'Yes' || s_105 == 'Yes') && !(s_98 == 'Yes' && s_1462== 'Yes') ",
          "steps": [
            {
              "id": "symptoms_worse",
             
              "type": "prompt",
              "data": {
                "type": "choice",
                "text": "Are your symptoms worsening rapidly",
                "options": [ "Yes", "No", "Dont Know" ]
              }
            },
            {
              "id": "breathing_fast",             
              "type": "prompt",
              "data": {
                "type": "choice",
                "text": "Are your breathing very fast?",
                "options": [ "Yes", "No", "Dont Know" ]
              }
            },
            {
              "id": "cough_blood",             
              "type": "prompt",
              "data": {
                "type": "choice",
                "text": "Are your coughing up blood?",
                "options": [ "Yes", "No", "Dont Know" ]
              }
            }
          ]
        }
      ]
    },
    {
      "id": "secondaryCovidSymptoms",
      "type": "sequence",
      "scenarios": [
        {
          "condition": "(!(temparature == 'Yes' || symptoms_worse == 'Yes' || cough_blood == 'Yes' || breathing_fast == 'Yes' ) && !(s_98 == 'Yes' && s_1462== 'Yes')) || !(s_98 == 'Yes' || s_1462== 'Yes' || s_105=='Yes')",
          "steps": [
            {
              "id": "secondaryCovidSynptomOptions",
             
              "type": "prompt",
              "data": {
                "type": "choice",
                "text": "Do you have any of the following symptoms? \n Feeling of weakness or Tiredness \n Muscle Pain \n Chills \n Headache \n Diarrhea \n Nausea \n Sore throat",
                "options": [ "Yes", "No", "Dont Know" ]
              }
            },
            {
              "id": "CovidTravel",
              "type": "sequence",
              "scenarios": [
                {
                  "condition": "s_98 == 'Yes' && s_105 == 'Yes'",
                  "steps": [
                    {
                      "id": "travel",
                     
                      "type": "prompt",
                      "data": {
                        "type": "choice",
                        "text": "Have you traveled or resided in the country of local, widespread transmission of coronavirus?",
                        "options": [ "Yes", "No" ]
                      }
                    }

                  ]
                }
              ]
            },
            {
              "id": "CovidContact",
              "type": "sequence",
              "scenarios": [
                {
                  "condition": "travel != 'Yes'",
                  "steps": [
                    {
                      "id": "contact",
                     
                      "type": "prompt",
                      "data": {
                        "type": "choice",
                        "text": "Have you had close contact with a person with suspected COVIF-19 infection in the last 14 days? By suspected, I mean a person with a confirmed COVID-19 infection, or tested for COVID-19",
                        "options": [ "Yes", "No" ]
                      }
                    }

                  ]
                }
              ]
            }            
          ]
        }
      ]
    },
    {
      "id": "firstAndLastName",
      "type": "prompt",
      "data": {
        "type": "text",
        "text": "I am ready with my diagnosis for COVID-19. I just need your name and contact details\n\n. **Please enter your name**"
      }
    },
    {
      "id": "mobileNumber",
      "type": "prompt",
      "data": {
        "type": "text",
        "text": "Please enter your 10 digit mobile number including the area code.",
        "validation": {
          "type": "regex",
          "setup": {
            "pattern": "^[+]?([0-9]*[\\.\\s\\-\\(\\)]|[0-9]+){10,13}$"
          }
        }
      }
    },
    {
      "id": "CovidInitialDiagnosis",
      "type": "sequence",
      "scenarios": [
        {
          "condition": "(s_98 == 'Yes' && s_1462== 'Yes') || (temparature == 'Yes' || symptoms_worse == 'Yes' || cough_blood == 'Yes' || breathing_fast == 'Yes')",
          "steps": [
            {
              "id": "initialCovidResult001",
             
              "type": "text",
              "data": {
                "type": "text",
                "text": "Corona Virus Alert. Call Emergency Number. Avoid all contact"
              }
            }
          ]
        },
       
        {
          "condition": "!(s_98 == 'Yes' || s_1462== 'Yes' || s_105=='Yes') && (contact == 'No') ",
          "steps": [
            {
              "id": "initialCovidResult002",
             
              "type": "text",
              "data": {
                "type": "text",
                "text": "Corona Virus Alert. Follow preventive measures"
              }
            }
          ]
        },
        {
          "condition": "!(s_98 == 'Yes' || s_1462== 'Yes' || s_105=='Yes') && (contact == 'Yes') ",
          "steps": [
            {
              "id": "initialCovidResult003",
             
              "type": "text",
              "data": {
                "type": "text",
                "text": "Corona Virus Alert. Quarantine Yourself"
              }
            }
          ]
        },
        {
          "condition": "travel=='Yes'",
          "steps": [
            {
              "id": "initialCovidResult004",
             
              "type": "text",
              "data": {
                "type": "text",
                "text": "Corona Virus Alert. Consult healthcare provider. Avoid all Contact"
              }
            }
          ]
        },
        {
          "condition": "(s_98 == 'Yes' || s_1462== 'Yes' || s_105=='Yes') && contact == 'Yes'",
          "steps": [
            {
              "id": "initialCovidResult005",
             
              "type": "text",
              "data": {
                "type": "text",
                "text": "Corona Virus Alert. Consult health department. Avoid all Contact"
              }
            }
          ]
        },
        {
          "condition": "(s_98 == 'Yes' || s_1462== 'Yes' || s_105=='Yes') && contact=='No'",
          "steps": [
            {
              "id": "initialCovidResult006",
             
              "type": "text",
              "data": {
                "type": "text",
                "text": "Corona Virus Alert. Stay home and monitor your symptoms"
              }
            }
          ]
        }

      ]
    },

    {
      "id": "continue_diagnosis",
      "type": "sequence",
      "steps": [
        {
          "id": "cont_diag",
          "type": "prompt",
          "data": {
            "type": "confirm",
            "text": "I can continue the interview and provide an enhanced diagnois report, completely free! do you want to continue?"
          },
          "scenarios": [
            {
              "condition": "cont_diag",
              "steps": [
                {
                  "id": "thanks_cont",
                  "type": "text",
                  "data": {
                    "type": "text",
                    "text": "Great! Lets Continue."
                  }
                }
              ]
            },
            {
              "condition": "!cont_diag",
              "steps": [
                {
                  "id": "sureNotContDiag",
                  "type": "prompt",
                  "data": {
                    "type": "confirm",
                    "text": "Are you sure?"
                  },
                  "scenarios": [
                    {
                      "condition": "!sureNotContDiag",
                      "type": "text",
                        "data": {
                          "type": "text",
                          "text": "Ok. Let's Continue!"
                        }
                    },
                    {
                      "condition": "sureNotContDiag",
                      "nodeId": "welcomeCard"
                    }
                  ]
                }
                
              ]
            }
          ]
        }
      ]
     
    },
    {
      "id": "height",
      "type": "prompt",
      "data": {
        "type": "number",
        "text": "Enter your height (in Inches)"
      }
    },
    {
      "id": "weight",
      "type": "prompt",
      "data": {
        "type": "number",
        "text": "Enter your weight (in pounds)"
      }
    },
   
    {
      "id": "p_8",
      "type": "prompt",
      "data": {
        "type": "choice",
        "text": "Do you have diabetes?",
        "options": [ "Yes", "No", "Dont Know" ]
      }
    },
    {
      "id": "p_9",
      "type": "prompt",
      "data": {
        "type": "choice",
        "text": "Do you suffer from hypetension?",
        "options": [ "Yes", "No", "Dont Know" ]
      }
    },
   
    {
      "id": "complaint",
      "type": "prompt",
      "data": {
        "type": "text",
        "text": "Do you have any other complaints?  \n You can enter phrases like **I have headache and body pain** "
      }
    },
    {
      "id": "initialSymptomsProcess001",
      "type": "initialSymptomsProcess",
      "data": { "mobileNumber": "<%mobileNumber%>" }
    },

    {
      "id": "initiateDiagnosisInterview001",
      "type": "initiateDiagnosisInterview",
      "data": { "mobileNumber": "<%mobileNumber%>" }
    },





    {
      "id": "nextQuestion",
      "type": "sequence",
      "scenarios": [
        {
          "condition": "should_stop == false",
          "steps": [
            {
              "id": "luisretry.false",
              "type": "prompt",
              "data": {
                "type": "choice",
                "text": "<%next_question%>",
                "options": [ "Yes", "No", "Dont Know" ]
              }
            },
            {
              "id": "processDiagnosisResponse002",
              "type": "processDiagnosisResponse",
              "data": { "mobileNumber": "<%mobileNumber%>" },
              "scenarios": [
                {
                  "condition": "should_stop == false",
                  "nodeId": "nextQuestion"
                },

                {
                  "condition": "should_stop == true",
                  "steps": [
                    {
                      "id": "ready_diagnosis",
                      "type": "text",
                      "data": {
                        "text": "I am ready with my dignosis report"
                      }
                    },
                   

                    {
                      "id": "luisretry.true",
                      "type": "text",
                      "data": {
                        "text": "**Conditions -**\n\n<%conditions%>"
                      }
                    }

                  ]
                }
              ]
            }

          ]
        }
      ]
    },

    {
      "id": "nextStep",
      "type": "sequence",
      "steps": [
        {
          "id": "talkToDoc",
          "type": "prompt",
          "data": {
            "type": "confirm",
            "text": "Do you want to Chat with a Doc?"
          },
          "scenarios": [
            {
              "condition": "talkToDoc",
              "steps": [
                {
                  "id": "docTime",
                  "type": "prompt",
                  "data": {
                    "type": "time",
                    "text": "What's the best time for the docor to call you today?"
                  }
                },
                {
                  "id": "docTimeThanks",
                  "type": "text",
                  "data": {
                    "text": "Thank you! I'll have the doctor call you asap!"
                  },
                  "scenarios": [
                    {
                      "condition": "talkToDoc",
                      "nodeId": "welcomeCard"
                    }
                  ]
                }


              ]
            },
            {
              "condition": "!talkToDoc",
              "steps": [
                {
                  "id": "sureNotTalkingToDoc",
                  "type": "prompt",
                  "data": {
                    "type": "confirm",
                    "text": "Are you sure?"
                  },
                  "scenarios": [
                    {
                      "condition": "!sureNotTalkingToDoc",
                      "nodeId": "talkToDoc"
                    },
                    {
                      "condition": "sureNotTalkingToDoc",
                      "nodeId": "welcomeCard"
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}